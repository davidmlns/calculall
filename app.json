{
  "expo": {
    "name": "Calculall",
    "slug": "Calculall",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-without-bg.png",
    "scheme": "calculall",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon-without-bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1A1A"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1A1A1A"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-without-bg.png",
          "imageWidth": 50,
          "resizeMode": "contain",
          "backgroundColor": "#1A1A1A"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
